/**app.wxss**/
page {
  height: 100%;
  box-sizing: border-box;
  font-size: 36rpx;
  font-family: SimHei;
} 
.btn{
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  background: #f85959;
  font-size: 36rpx;
  letter-spacing: 10rpx;
  margin: auto;
}
.success-img{
  width: 208rpx;
  height: 220rpx;
}
.fail-img{
  width: 226rpx;
  height: 259rpx;
}
.welcom-text{
  width: 456rpx;
  height: 60rpx;
}
.continue-img{
  width: 254rpx;
  height: 252rpx;
}

.pro .list{
  background: #fff;
  padding: 30rpx 28rpx;
  margin: 17rpx 32rpx 0 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.pro .list view:nth-of-type(1){
  font-size: 34rpx;
}
.pro .list view:nth-child(2){
  margin-top: 10rpx;
}
.list-flex{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.pro .list text{
  color: #999;
  font-size: 24rpx;
  margin-right: 20rpx;
}
.pro .list text:nth-last-of-type(1){
  margin-right: 0;
}
.top{
	background-color: #ff0000;
  font-size: 22rpx!important;
  color: #fff;
  text-align: center;
  padding: 5rpx 8rpx;
  display: inline-block;
  border-radius: 5rpx;
  margin-right: 20rpx;
}
.picker{
  background: url(data:image/png;base64,) right center no-repeat;
  padding-right: 30rpx;
  /* width: 8.5rpx; */
  /* height: 15rpx; */
  /* display: block; */
  background-size:19rpx ;
}
